<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="false">
    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_gravity="center"
        android:columnCount="3"
        android:orientation="horizontal"
        android:rowCount="3">

        <ImageButton
            android:id="@+id/sq_1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic00"/>

        <ImageButton
            android:id="@+id/sq_2"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic01"/>

        <ImageButton
            android:id="@+id/sq_3"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic02"/>

        <ImageButton
            android:id="@+id/sq_4"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic10"/>

        <ImageButton
            android:id="@+id/sq_5"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/empty"/>

        <ImageButton
            android:id="@+id/sq_6"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic12"/>

        <ImageButton
            android:id="@+id/sq_7"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic20"/>

        <ImageButton
            android:id="@+id/sq_8"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic21"/>

        <ImageButton
            android:id="@+id/sq_9"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/tic22"/>
    </GridLayout>
</FrameLayout>
